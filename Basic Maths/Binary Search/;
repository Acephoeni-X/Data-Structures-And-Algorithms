#include <iostream>

using namespace std;

int recur(int *arr, int x, int start, int end){
  int mid = (start+end)/2;
  if(arr[mid] == x){
    if(arr[mid]-1 == x){
      end = mid-1;
    }
    else
      return midl
  }
  else if (x > arr[mid])
    start = mid + 1;
  else 
    end = mid -1;
}

int main(){
  int n, arr[100], x;
  cin >> n;
  for(int i=0; i<n; i++)
    cin >> arr[i];
  cin >> x;
  cout << recur(arr, x, 0, n-1);
  return 0;
}

